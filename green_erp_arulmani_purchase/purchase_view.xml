<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    
		<record id="view_gate_in_pass_tree" model="ir.ui.view">
			<field name="name">gate.in.pass.tree</field>
			<field name="model">gate.in.pass</field>
			<field name="arch" type="xml">
				<tree string="Gate In Pass">
					<field name="name" />
					<field name="po_number"/>
					<field name="po_date"/>
					<field name="supplier_id" />
					<field name="gate_date_time"/>
				</tree>
			</field>
		</record>
		
		<record id="view_gate_in_pass_form" model="ir.ui.view">
            <field name="name">gate.in.pass.form</field>
            <field name="model">gate.in.pass</field>
            <field name="arch" type="xml">
	            <form string="Gate In Pass" version="7.0">   
	            	<sheet>
		                <group > 
		                <field name="po_number"/>
		                <field name="supplier_id"/>
		                <field name="po_date"/>
						<field name="name"/>
						<field name="gate_date_time"/>
						</group>
				    </sheet>
		         </form>
			</field>
		</record>
    </data>
</openerp>
