<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
	    <record id="tpt_reject_mrr_form_view" model="ir.ui.view">
	        <field name="name">tpt.reject.mrr</field>
	        <field name="model">tpt.reject.mrr</field>
	        <field eval="20" name="priority"/>
	        <field name="arch" type="xml">
	            <form string="Alert" version="7.0">
	            	<group>
		                <field name="name" placeholder="Please type the reason" required='1'/>
		            </group>
	                <footer>
	                    <button name='bt_reject' type='object' string="Reject" class="oe_highlight"/>
	                    or
	                    <button string="Cancel" special="cancel" />
	                </footer>
	            </form>
	        </field>
    	</record>

    </data>
</openerp>

